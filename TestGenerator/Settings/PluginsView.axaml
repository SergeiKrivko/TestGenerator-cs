<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:settings="clr-namespace:TestGenerator.Settings"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="TestGenerator.Settings.PluginsView">
    <TabControl>
        <TabItem Header="Установленные">
            <settings:LocalPluginsList></settings:LocalPluginsList>
        </TabItem>
        <TabItem Header="Доступные">
            <settings:RemotePluginsList></settings:RemotePluginsList>
        </TabItem>
    </TabControl>
</UserControl>
