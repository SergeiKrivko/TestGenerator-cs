<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Shared.Settings.Shared.FileItem">
    <StackPanel Orientation="Horizontal" Spacing="5" Margin="25 0 0 0">
        <CheckBox Name="CheckBox" VerticalAlignment="Center" IsCheckedChanged="CheckBox_OnIsCheckedChanged"></CheckBox>
        <TextBlock Name="NameBlock" VerticalAlignment="Center"></TextBlock>
    </StackPanel>
</UserControl>